<%= form_for([@listing, @order]) do |f| %>
  <% if order.errors.any? %>
     <div id="error_explanation" class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <h4><%= pluralize(order.errors.count, "error") %> prohibited this order from being saved:</h4>

      <ul>
      <% order.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :emailaddress %>
    <%= f.text_field :emailaddress, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :cellphonenumber %>
    <%= f.text_field :cellphonenumber, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :cellphonecarrier %>
    <%= f.text_field :cellphonecarrier, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit "Complete Order", class: "btn btn-success" %>
  </div>
<% end %>
