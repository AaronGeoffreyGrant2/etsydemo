<p id="notice"><%= notice %></p>

<h1>Obstacledodgergames</h1>

<table>
  <thead>
    <tr>
      <th>Dodgerimage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obstacledodgergames.each do |obstacledodgergame| %>
      <tr>
        <td><%= obstacledodgergame.dodgerimage %></td>
        <td><%= link_to 'Show', obstacledodgergame %></td>
        <td><%= link_to 'Edit', edit_obstacledodgergame_path(obstacledodgergame) %></td>
        <td><%= link_to 'Destroy', obstacledodgergame, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obstacledodgergame', new_obstacledodgergame_path %>
